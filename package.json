{
  "name": "start-api-pub",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel src -d dist",
    "postbuild": "node dist/index.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "start": "DEBUG=app npm-run-all --parallel lint:watch test-unit:watch start-watch",
    "start-watch": "nodemon src/index.js --exec babel-node",
    "start-prod": "babel src -d dist && node dist/index.js",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm-run-all test-unit test-app",
    "test-unit": "mocha --reporter progress \"src/**/**.test.js\" --require @babel/register --require @babel/polyfill",
    "test-unit:watch": "npm run test-unit -- --watch",
    "test-app": "mocha --reporter progress \"src/**/**.test-app.js\" --require @babel/register --require @babel/polyfill --exit",
    "heroku-create": "heroku create",
    "heroku-remote": "heroku git:remote -a sleepy-dusk-61958",
    "heroku-deploy": "git push heroku master",
    "heroku-mongolab-add": "heroku addons:create mongolab # Created mongolab-name as MONGODB_URI",
    "heroku-mongolab-docs": "heroku addons:docs mongolab",
    "heroku-config-vars": "heroku config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anturson/start-api-pub.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anturson/start-api-pub/issues"
  },
  "homepage": "https://github.com/anturson/start-api-pub#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chai-spies": "^1.0.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "numeral": "^2.0.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "faker": "^4.1.0",
    "json-schema-faker": "^0.5.0-rc16",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  }
}
